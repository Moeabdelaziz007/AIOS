10 REM Test Mathematical Operations
20 PRINT "Starting Mathematical Operations Test"
30 LET A = 15
40 LET B = 3
50 PRINT "A = "; A; ", B = "; B
60 PRINT "A + B = "; A + B
70 PRINT "A - B = "; A - B
80 PRINT "A * B = "; A * B
90 PRINT "A / B = "; A / B
100 PRINT "A MOD B = "; A MOD B
110 LET C = A ^ 2
120 PRINT "A squared = "; C
130 LET D = SQR(A)
140 PRINT "Square root of A = "; D
150 PRINT "Mathematical Operations Test Complete"
160 END
